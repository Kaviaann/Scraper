for(let i of $('td')){
    for(let o of i.children.filter(v => v.type === "tag")){
      switch(o.name){

        case 'a':{
          data.name = o.children[0].data
          break;
        }

        case 'div':{
          switch(o.attribs.class){

            case 'picSurround':{
              const thumbEl = o.children[0]
              data.link = thumbEl.attribs.href
              data.thumb = thumbEl.children[0].attribs['data-src']
              break;
            }

          }
        }
        break;
      }
    }
  }